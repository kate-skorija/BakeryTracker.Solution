@{
  Layout = "_Layout";
}

<h2>Add a Vendor</h2>
<p>Fill out the form below to add a vendor to your list.</p>

<form action="/vendors" method="post">
  <label for="vendorName">Vendor Name:</label>
  <input id="vendorName" name="vendorName" type="text"><br><br>
  <button type="submit" class="btn btn-dark">Add Vendor</button>
</form>